-  private void updateClippingToRect(Rect clippingRect) {
-    Assertions.assertNotNull(mAllChildren);
-    int clippedSoFar = 0;
-    for (int i = 0; i < mAllChildrenCount; i++) {
-      updateSubviewClipStatus(clippingRect, i, clippedSoFar);
